{
  "name": "noteworthy",
  "version": "1.5.3",
  "description": "Noteworthy is an easy-to-use note taking app designed to be simplistic, yet powerful.\\n\\nFor more information, visit this project at www.adeolauthman.com/projects/Noteworthy.",
  "main": "main.js",
  "scripts": {
    "clean": "rm -rf dist .cache && rm -rf -R Noteworthy-darwin-x64",
    "test": "parcel -p 3000 src/index.html",
    "package": "parcel build src/index.html --out-dir dist && cp netlify.toml dist/netlify.toml",
    "start": "electron .",
    "build-MacOS": "electron-packager . Noteworthy --asar=true && cp NoteworthyAppLogo.icns Noteworthy-darwin-x64/Noteworthy.app/Contents/Resources/electron.icns",
    "build-Windows": "electron-packager . Noteworthy --platform=win32 --overwrite --arch=ia32 --asar=true --icon=NoteworthyAppLogo.icns --prune=true --out=release-builds",
    "electron-toolkit": "electron ./node_modules/electron-toolkit",
    "dist": "electron-builder --x64 --mac",
    "deploy": "npm run package && git add . && git commit -m \"new version\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Authman2/Noteworthy.git"
  },
  "keywords": [
    "noteworthy",
    "note",
    "taking",
    "frontend",
    "mosaic"
  ],
  "author": "Adeola Uthman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Authman2/Noteworthy/issues"
  },
  "homepage": "https://github.com/Authman2/Noteworthy#readme",
  "dependencies": {
    "@authman2/mosaic": "^0.5.5",
    "babel-polyfill": "^6.26.0",
    "firebase": "^5.9.0",
    "jquery": "^3.3.1",
    "moment": "^2.24.0",
    "superagent": "^5.0.3",
    "tippy.js": "^4.2.1",
    "turndown": "^5.0.3"
  },
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-builder": "^20.39.0",
    "electron-packager": "^13.1.1",
    "electron-toolkit": "^1.0.24",
    "less": "^3.9.0",
    "parcel-bundler": "^1.12.3"
  },
  "build": {
    "appId": "com.adeolauthman.Noteworthy",
    "copyright": "Copyright Â© 2018 Adeola Uthman",
    "productName": "Noteworthy",
    "electronVersion": "1.6.15",
    "asar": true,
    "directories": {
      "app": "/Users/adeolauthman/Documents/AdeolasCodingStuff/WebDevelopment/Noteworthy",
      "output": "dist"
    },
    "mac": {
      "asar": "true",
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    },
    "publish": {
      "provider": "github",
      "repo": "https://github.com/Authman2/Noteworthy",
      "owner": "Adeola Uthman",
      "token": "c527b4b6ef70fcdd2059ad8d9cd5ecbb08749fd9",
      "vPrefixedTagName": true,
      "releaseType": "release",
      "private": true
    }
  }
}
